{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CodeSnippet",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "desc": {
      "type": "string"
    },
    "lang": {
      "type": "string",
      "enum": ["py", "c", "js", "ts", "cpp", "java", "sql", "bash", "psl"]
    },
    "code": {
      "type": "string"
    },
    "tag": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["title", "lang", "code"],
  "additionalProperties": false
}
